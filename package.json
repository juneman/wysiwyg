{
  "name": "wysiwyg",
  "version": "1.19.7",
  "description": "Appcues Editor",
  "main": "lib/bundle.js",
  "scripts": {
    "test": "NODE_ENV=test nyc mocha --reporter=dot",
    "test-only": "NODE_ENV=test mocha --reporter=spec",
    "coverage-report": "NODE_ENV=test nyc --reporter=html mocha --reporter=dot",
    "lint": "eslint .",
    "posttest": "npm run lint",
    "build": "npm run clean && babel src -d lib && NODE_ENV=production webpack --config webpack.build.js",
    "watch": "npm run clean && babel src -d lib && webpack --config webpack.build.js --watch",
    "start": "NODE_ENV=development webpack-dev-server --config webpack.demo.js --hot --inline --colors",
    "clean": "rm -rf lib && mkdir lib"
  },
  "author": "Michael Cox",
  "license": "MIT",
  "dependencies": {
    "brace": "^0.11.0",
    "draft-convert": "^2",
    "draft-js": "brandcast/draft-js-built#003f24b",
    "emoji-mart": "^2.4.2",
    "enzyme-adapter-react-16": "^1.1.1",
    "html-parse-stringify2": "^2.0.1",
    "immutable": "^3.8.1",
    "lodash": "^4.17.4",
    "node-object-hash": "^1.2.0",
    "prop-types": "^15.5.8",
    "react-color": "^2.11.4",
    "react-dnd": "^2.5.0",
    "react-dnd-html5-backend": "^2.5.0",
    "react-dropzone": "^4.2.7",
    "react-redux": "^5.0.4",
    "react-simple-range": "appcues/react-simple-range",
    "reactcss": "^1.2.3",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "sanitize-html": "^1.14.1",
    "striptags": "^3.1.1",
    "tinycolor2": "^1.4.1",
    "uuid": "^3.0.1"
  },
  "peerDependencies": {
    "react": "brandcast/react-built#efc513",
    "react-dom": "brandcast/react-dom-built#309731"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-istanbul": "^4.1.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-3": "^6.24.1",
    "babel-register": "^6.24.0",
    "chai": "^4.1.2",
    "chai-enzyme": "^1.0.0-beta.0",
    "chai-immutable": "^1.6.0",
    "copy-webpack-plugin": "^4.4.1",
    "enzyme": "^3.3.0",
    "eslint": "^4.17.0",
    "eslint-plugin-react": "^7.6.1",
    "jsdom": "^11.6.2",
    "mocha": "^5.0.0",
    "nyc": "^11.4.1",
    "path": "^0.12.7",
    "react": "brandcast/react-built#efc513",
    "react-dom": "github:brandcast/react-dom-built#309731",
    "redux-devtools-extension": "^2.13.0",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.2.3",
    "sinon": "^4.3.0",
    "sinon-chai": "^2.10.0",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.4.2"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  }
}
