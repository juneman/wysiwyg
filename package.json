{
  "name": "wysiwyg",
  "version": "1.19.1",
  "description": "Appcues Editor",
  "main": "lib/bundle.js",
  "scripts": {
    "test": "NODE_ENV=test nyc mocha --reporter=dot",
    "test-only": "NODE_ENV=test mocha --reporter=spec",
    "coverage-report": "NODE_ENV=test nyc --reporter=html mocha --reporter=dot",
    "lint": "eslint .",
    "posttest": "npm run lint",
    "build": "npm run clean && babel src -d lib && NODE_ENV=production webpack --config webpack.build.js",
    "watch": "npm run clean && babel src -d lib && webpack --config webpack.build.js --watch",
    "start": "NODE_ENV=development webpack-dev-server --config webpack.build.js --hot --inline --colors",
    "clean": "rm -rf lib && mkdir lib"
  },
  "author": "Michael Cox",
  "license": "MIT",
  "dependencies": {
    "brace": "^0.10.0",
    "draft-convert": "^2",
    "draft-js": "github:appcues/draft-js",
    "emoji-mart": "^1.0.1",
    "html-parse-stringify2": "^2.0.1",
    "immutable": "^3.8.1",
    "node-object-hash": "^1.2.0",
    "prop-types": "^15.5.8",
    "react-ace": "^4.2.1",
    "react-color": "^2.11.4",
    "react-dnd": "^2.3.0",
    "react-dnd-html5-backend": "^2.3.0",
    "react-dropzone": "^3.12.4",
    "react-redux": "^5.0.4",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "sanitize-html": "^1.14.1",
    "striptags": "^2.2.1",
    "tinycolor2": "^1.4.1",
    "uuid": "^3.0.1"
  },
  "peerDependencies": {
    "react": "^15.5.4",
    "react-dom": "^15.5.4"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.1",
    "babel-loader": "^6.4.1",
    "babel-plugin-istanbul": "^4.1.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-register": "^6.24.0",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.6.1",
    "chai-immutable": "^1.6.0",
    "copy-webpack-plugin": "^4.0.1",
    "enzyme": "^2.8.1",
    "eslint": "^3.19.0",
    "eslint-plugin-react": "^6.10.3",
    "jsdom": "^9.12.0",
    "mocha": "^3.2.0",
    "nyc": "^10.2.0",
    "path": "^0.12.7",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-test-renderer": "^15.5.4",
    "redux-devtools-extension": "^2.13.0",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.2.3",
    "sinon": "^2.2.0",
    "sinon-chai": "^2.10.0",
    "webpack": "^2.3.3",
    "webpack-dev-server": "^2.4.2"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  }
}
